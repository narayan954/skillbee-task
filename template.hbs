<html>
  <head>
    <title>{{type}} Page</title>
    <meta name="description" content="{{activity}}" />
    {{!-- add css stylesheet --}}
    <link rel="stylesheet" href="page.css">
  </head>
  <body>
    <div class="app">
      <div class="left">
        <div class="key">key: {{key}}</div>
        <div class="activity">{{activity}}</div class="activity">
        {{!-- explore more button --}}
        <div class="explore">
          <a href="{{link}}" >Explore More</a>
        </div>
        <footer>
          <div class="accessibility">
            <div class="footer-heading">accessibility:</div>
            <div class="footer-text">{{accessibility}}</div>
          </div>
          <div class="type">
            <div class="footer-heading"> Type:</div>
            <div class="footer-text">{{type}}</div>
          </div>     
        </footer>
      </div>
      <div class="right">
        <div class="price">
          <div class="price-heading"> Price: </div>
          <div class="price-text">{{price}}</div>
        </div>
        <div class="img-grid">
          <div class="img1">img1</div>
          <div class="img2">img2</div>
          <div class="img3">img3</div>
        </div>
      </div>
    </div>   
  </body>
</html>